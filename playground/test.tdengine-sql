drop database if exists scd;
create database if not exists scd;
use scd;
create table stb1 (ts timestamp, c1 bool, c2 tinyint, c3 smallint, c4 int, c5 bigint, c6 float,
	c7 double, c8 binary(10), c9 nchar(10), c10 tinyint unsigned, c11 smallint unsigned,
	c12 int unsigned, c13 bigint unsigned) TAGS(t1 int, t2 binary(10), t3 double);

create table tb1 using stb1 tags(1,'1',1.0);

create table tb2 using stb1 tags(2,'2',2.0);

create table tb3 using stb1 tags(3,'3',3.0);

create database scd2 stt_trigger 3;

create database scd4 stt_trigger 13;

show create database scd;
CREATE DATABASE `scd` BUFFER 256 CACHESIZE 1 CACHEMODEL 'none' COMP 2
	DURATION 14400m WAL_FSYNC_PERIOD 3000 MAXROWS 4096 MINROWS 100
	STT_TRIGGER 2 KEEP 5256000m,5256000m,5256000m PAGES 256 PAGESIZE 4
	PRECISION 'ms' REPLICA 1 WAL_LEVEL 1 VGROUPS 2 SINGLE_STABLE 0
	TABLE_PREFIX 0 TABLE_SUFFIX 0 TSDB_PAGESIZE 4 WAL_RETENTION_PERIOD 3600
	WAL_RETENTION_SIZE 0;

